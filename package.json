{
    "name"       : "vue-test",
    "version"    : "0.1.0",
    "description": "Test for Vue",
    "author"     : "gfoidl",
    "license"    : "(c) gfoidl, 2020",
    "private"    : true,
    "scripts": {
        "build"                 : "webpack --mode=development",
        "build-watch"           : "webpack --mode=development --watch",
        "build-release"         : "webpack --mode=production",
        "start"                 : "webpack-dev-server --open chrome --hotOnly",
        "test"                  : "jest",
        "test-unit"             : "jest --projects tests/unit --verbose",
        "test-vue"              : "jest --projects tests/vue --verbose",
        "test-e2e"              : "jest --projects tests/e2e --verbose --reporters=default --reporters=jest-junit",
        "test-coverage"         : "jest --verbose --reporters=default --reporters=jest-junit --coverage",
        "alsatian-test"         : "alsatian tests/unit2/**/*.ts",
        "alsatian-test-results" : "alsatian tests/unit2/**/*.ts --tap | tap-junit -o out/tests -n test-results.xml",
        "alsatian-test-coverage": "nyc --reporter=cobertura --reporter=html yarn run unit-test && nyc report"
    },
    "dependencies": {
        "bootstrap"    : "^4.4.1",
        "bootstrap-vue": "^2.13.0",
        "vue"          : "^2.6.11"
    },
    "devDependencies": {
        "@babel/core"                       : "7.0.0",
        "@types/expect-puppeteer"           : "^4.4.1",
        "@types/jest"                       : "^25.2.1",
        "@types/jest-environment-puppeteer" : "^4.3.1",
        "@types/puppeteer"                  : "^2.0.1",
        "@vue/test-utils"                   : "1.0.0-beta.33",
        "alsatian"                          : "^3.2.1",
        "babel-core"                        : "^7.0.0-bridge.0",
        "clean-webpack-plugin"              : "^3.0.0",
        "css-loader"                        : "^3.5.3",
        "file-loader"                       : "^6.0.0",
        "fork-ts-checker-webpack-plugin"    : "^4.1.3",
        "git-revision-webpack-plugin"       : "^3.0.6",
        "html-webpack-plugin"               : "^4.2.1",
        "jest"                              : "^25.5.4",
        "jest-junit"                        : "^10.0.0",
        "jest-puppeteer"                    : "^4.4.0",
        "less"                              : "^3.11.1",
        "less-loader"                       : "^6.0.0",
        "mini-css-extract-plugin"           : "^0.9.0",
        "mini-svg-data-uri"                 : "^1.1.3",
        "nyc"                               : "^15.0.1",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "preload-webpack-plugin"            : "3.0.0-beta.4",
        "puppeteer"                         : "^2.1.1",
        "serve"                             : "^11.3.0",
        "style-loader"                      : "^1.0.0",
        "tap-junit"                         : "^3.1.2",
        "terser-webpack-plugin"             : "^2.1.2",
        "tsconfig-paths"                    : "^3.9.0",
        "tsconfig-paths-webpack-plugin"     : "^3.2.0",
        "ts-jest"                           : "^25.4.0",
        "ts-loader"                         : "^7.0.1",
        "tslib"                             : "^1.11.1",
        "typescript"                        : "^3.8.3",
        "url-loader"                        : "^4.1.0",
        "vue-class-component"               : "^7.1.0",
        "vue-jest"                          : "^3.0.5",
        "vue-loader"                        : "^15.7.1",
        "vue-property-decorator"            : "^8.2.2",
        "vue-template-compiler"             : "^2.6.10",
        "webpack"                           : "^4.41.0",
        "webpack-bundle-analyzer"           : "^3.7.0",
        "webpack-cli"                       : "^3.3.11",
        "webpack-dev-server"                : "^3.8.1",
        "webpack-pwa-manifest"              : "^4.2.0",
        "workbox-core"                      : "^5.1.3",
        "workbox-precaching"                : "^5.1.3",
        "workbox-webpack-plugin"            : "^5.1.3"
    }
}
